{
  "name": "@versatiles/versatiles-rs",
  "version": "3.7.0",
  "description": "Node.js bindings for VersaTiles - convert, serve, and process map tiles",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    "types": "./index.d.ts",
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "index.cjs"
  ],
  "napi": {
    "binaryName": "versatiles",
    "targets": [
      "aarch64-apple-darwin",
      "x86_64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "x86_64-pc-windows-msvc"
    ]
  },
  "keywords": [
    "versatiles",
    "mbtiles",
    "pmtiles",
    "tiles",
    "map",
    "gis",
    "geo",
    "rust"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versatiles-org/versatiles-rs.git",
    "directory": "versatiles_node"
  },
  "license": "MIT",
  "author": "Michael Kreil <versatiles@michael-kreil.de>",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build:debug": "npm run build:cjs:debug && npm run build:esm:debug",
    "build:cjs:debug": "napi build --platform && mv index.js index.cjs",
    "build:esm:debug": "napi build --platform --esm",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "napi build --platform --release && mv index.js index.cjs",
    "build:esm": "napi build --platform --release --esm",
    "check": "npm run build:debug && npm run typecheck && npm run lint && npm run test && npm run test:examples && npm run format:check",
    "docs:clean": "rm -rf docs",
    "docs:generate": "typedoc",
    "docs:serve": "npx http-server docs -s -c 0 -p 8080 -o",
    "fix": "npm run lint:fix && npm run format",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "pack:dry": "npm pack --dry-run",
    "prepublishOnly": "napi prepublish -t npm",
    "test:coverage": "vitest run --coverage",
    "test:examples": "npx tsx scripts/run-examples.ts",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "upgrade": "npm-check-updates -u && npm install",
    "version": "napi version"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@napi-rs/cli": "^3.5.1",
    "@types/node": "^25.3.0",
    "@vitest/ui": "^4.0.18",
    "chalk": "^5.6.2",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "npm-check-updates": "^19.5.0",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.17",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "@versatiles/versatiles-rs-darwin-arm64": "3.7.0",
    "@versatiles/versatiles-rs-darwin-x64": "3.7.0",
    "@versatiles/versatiles-rs-linux-arm64-gnu": "3.7.0",
    "@versatiles/versatiles-rs-linux-arm64-musl": "3.7.0",
    "@versatiles/versatiles-rs-linux-x64-gnu": "3.7.0",
    "@versatiles/versatiles-rs-linux-x64-musl": "3.7.0",
    "@versatiles/versatiles-rs-win32-arm64-msvc": "3.7.0",
    "@versatiles/versatiles-rs-win32-x64-msvc": "3.7.0"
  }
}
